{
  "name": "burger-social",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:demo-content\" \"npm run dev:frontend\"",
    "dev:demo-content": "cd demo-content && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "cd demo-content && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
